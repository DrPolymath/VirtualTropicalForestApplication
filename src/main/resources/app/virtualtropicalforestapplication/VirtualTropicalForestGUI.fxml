<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="670.0" prefWidth="900.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.virtualtropicalforestapplication.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <StackPane prefHeight="375.0" prefWidth="818.0">
      <children>
         <ImageView fx:id="IVForest" fitHeight="519.0" fitWidth="857.0" pickOnBounds="true" preserveRatio="true" />
         <Pane prefHeight="200.0" prefWidth="200.0">
            <children>
               <ImageView fx:id="IVTiger" fitHeight="150.0" fitWidth="200.0" layoutX="411.0" layoutY="233.0" pickOnBounds="true" preserveRatio="true" x="80.0" y="20.0" />
               <ImageView fx:id="IVWolf" fitHeight="172.0" fitWidth="230.0" layoutX="109.0" layoutY="183.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVEagle" fitHeight="110.0" fitWidth="149.0" layoutX="440.0" layoutY="64.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVFrog" fitHeight="33.0" fitWidth="53.0" layoutX="56.0" layoutY="377.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVSnake" fitHeight="56.0" fitWidth="182.0" layoutX="452.0" layoutY="365.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVCrocodile" fitHeight="192.0" fitWidth="273.0" layoutX="224.0" layoutY="259.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVDuck" fitHeight="56.0" fitWidth="83.0" layoutX="140.0" layoutY="355.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVOwl" fitHeight="65.0" fitWidth="83.0" layoutX="624.0" layoutY="47.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVMonkey" fitHeight="104.0" fitWidth="130.0" layoutX="158.0" layoutY="59.0" pickOnBounds="true" preserveRatio="true" />
               <ImageView fx:id="IVSquirrel" fitHeight="48.0" fitWidth="63.0" layoutX="660.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true" />
            </children>
         </Pane>
      </children>
   </StackPane>
   <Pane prefHeight="200.0" prefWidth="200.0">
      <children>
          <Button layoutX="27.0" layoutY="21.0" onAction="#onCreateAnimals" text="Create Animals" />
      </children>
   </Pane>
</VBox>
